{
  "python.defaultInterpreterPath": "${workspaceFolder}/api/django_project/.venv/bin/python",
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.pyc": true,
    "**/*.pyo": true,
    "**/.git": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.venv": true,
    "**/dist": true,
    "**/node_modules/": true,
    "**/.next/": true,
    "**/.storybook/": true,
    "**/next-env.d.ts": true,
    "**/.ruff_cache/": true
  },
  "files.watcherExclude": {
    "**/.mypy_cache": true,
    "**/.venv": true,
    "**/docs/_build": true,
    "**/node_modules/": true,
    "**/.next/": true,
    "**/api/clients/": true
  },
  "python.testing.autoTestDiscoverOnSaveEnabled": true,
  "python.testing.cwd": "${workspaceFolder}/api/django_project/",
  "python.testing.pytestArgs": [
    "--rootdir=${workspaceFolder}/api/django_project/"
  ],
  "python.testing.debugPort": 8072,
  "debugpy.debugJustMyCode": true
}
